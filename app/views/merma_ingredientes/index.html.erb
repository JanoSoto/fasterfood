<h1>Listing merma_ingredientes</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th>Fecha merma</th>
      <th>Comentario merma</th>
      <th>Ingrediente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merma_ingredientes.each do |merma_ingrediente| %>
      <tr>
        <td><%= merma_ingrediente.cantidad %></td>
        <td><%= merma_ingrediente.fecha_merma %></td>
        <td><%= merma_ingrediente.comentario_merma %></td>
        <td><%= merma_ingrediente.ingrediente %></td>
        <td><%= link_to 'Show', merma_ingrediente %></td>
        <td><%= link_to 'Edit', edit_merma_ingrediente_path(merma_ingrediente) %></td>
        <td><%= link_to 'Destroy', merma_ingrediente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Merma ingrediente', new_merma_ingrediente_path %>
