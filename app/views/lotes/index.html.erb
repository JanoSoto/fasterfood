<h1>Listing lotes</h1>

<table>
  <thead>
    <tr>
      <th>Fecha hora llegada</th>
      <th>Fecha vencimiento</th>
      <th>Ingrediente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lotes.each do |lote| %>
      <tr>
        <td><%= lote.fecha_hora_llegada %></td>
        <td><%= lote.fecha_vencimiento %></td>
        <td><%= lote.ingrediente %></td>
        <td><%= link_to 'Show', lote %></td>
        <td><%= link_to 'Edit', edit_lote_path(lote) %></td>
        <td><%= link_to 'Destroy', lote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lote', new_lote_path %>
