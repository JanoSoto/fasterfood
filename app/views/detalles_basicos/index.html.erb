<h1>Listing detalles_basicos</h1>

<table>
  <thead>
    <tr>
      <th>Basico</th>
      <th>Ingrediente</th>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalles_basicos.each do |detalles_basico| %>
      <tr>
        <td><%= detalles_basico.basico_id %></td>
        <td><%= detalles_basico.ingrediente_id %></td>
        <td><%= detalles_basico.cantidad %></td>
        <td><%= link_to 'Show', detalles_basico %></td>
        <td><%= link_to 'Edit', edit_detalles_basico_path(detalles_basico) %></td>
        <td><%= link_to 'Destroy', detalles_basico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalles basico', new_detalles_basico_path %>
