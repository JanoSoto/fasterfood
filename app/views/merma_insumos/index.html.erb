<h1>Listing merma_insumos</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad merma</th>
      <th>Fecha merma</th>
      <th>Comentario merma</th>
      <th>Insumo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merma_insumos.each do |merma_insumo| %>
      <tr>
        <td><%= merma_insumo.cantidad_merma %></td>
        <td><%= merma_insumo.fecha_merma %></td>
        <td><%= merma_insumo.comentario_merma %></td>
        <td><%= merma_insumo.insumo %></td>
        <td><%= link_to 'Show', merma_insumo %></td>
        <td><%= link_to 'Edit', edit_merma_insumo_path(merma_insumo) %></td>
        <td><%= link_to 'Destroy', merma_insumo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Merma insumo', new_merma_insumo_path %>
