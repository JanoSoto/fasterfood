<h1>Listing detalles</h1>

<table>
  <thead>
    <tr>
      <th>Compuesto</th>
      <th>Basico</th>
      <th>Estado</th>
      <th>Tiempo entrega</th>
      <th>Tiempo en espera</th>
      <th>Tiempo preparando</th>
      <th>Tiempo terminado</th>
      <th>Tiempo entregado deseachado</th>
      <th>Intento entrega</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalles.each do |detalle| %>
      <tr>
        <td><%= detalle.compuesto_id %></td>
        <td><%= detalle.basico_id %></td>
        <td><%= detalle.estado %></td>
        <td><%= detalle.tiempo_entrega %></td>
        <td><%= detalle.tiempo_en_espera %></td>
        <td><%= detalle.tiempo_preparando %></td>
        <td><%= detalle.tiempo_terminado %></td>
        <td><%= detalle.tiempo_entregado_deseachado %></td>
        <td><%= detalle.intento_entrega %></td>
        <td><%= detalle.precio %></td>
        <td><%= link_to 'Show', detalle %></td>
        <td><%= link_to 'Edit', edit_detalle_path(detalle) %></td>
        <td><%= link_to 'Destroy', detalle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle', new_detalle_path %>
